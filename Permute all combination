class Solution {
    vector<vector<int>> result;
public:
    void permu(vector<int>& nums,int l,int h){
        
        if(l==h){
            result.push_back(nums);
        }
        for(int i=l;i<=h;i++){
            swap(nums[l],nums[i]);
            permu(nums,l+1,h);
            swap(nums[l],nums[i]);
        }
    }
    
    vector<vector<int>> permute(vector<int>& nums) {
        
        permu(nums,0,nums.size()-1);
        return result;
    }
};
